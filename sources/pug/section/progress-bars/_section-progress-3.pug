+section("section section-inset-2 bg-gray-900", "Skill bars", "col-sm-8 col-sm-offset-1 col-lg-6 col-lg.offset-custom-3")
	// Progress Bar
	+progress-bar-linear({
		title: "Dedication",
		valueTo: "50"
	})

	+progress-bar-linear({
		title: "Knowledge",
		valueTo: "90"
	})

	+progress-bar-linear({
		title: "Professionalism",
		valueTo: "80"
	})

	+progress-bar-linear({
		title: "Experience",
		valueTo: "20"
	})