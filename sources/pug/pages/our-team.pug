extends  ../_skeleton
include  ../mixin/_mixin-media

block variables
  - var activeIndex = 4
block title
  title Our team
block page
  // Page Header
  -var header_class = "bg-blog"
  -var header_h1 = "<span class='font-weight-bold'>Meet our People</span>"
  -var header_small = "Our Team"
  -var page_active = "Our team"
  -var page_parent = "Pages"
  include ../elements/_header-transparent
  // Page Content
  main.page-content
    include ../section/about/_section-breadcrumb
    include ../section/our-team/_section-our-team-2
    include ../section/our-team/_section-our-team-3
    include ../section/about/_section-team
    include ../section/our-team/_section-our-team-5
    include ../section/our-team/_section-our-team-6

  // Page Footer
  include ../elements/_footer-light-center
