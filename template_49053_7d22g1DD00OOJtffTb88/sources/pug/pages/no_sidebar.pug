extends  ../_skeleton
include ../mixin/_mixin-pagination
include ../mixin/_mixin-owl-testimonials
include ../mixin/_mixin-section
block variables
  - var activeIndex = 3
block title
  title Blog Without Sidebar
block page
  // Page Header
  -var header_class = "bg-blog"
  -var header_h1 = "<span class='font-weight-bold'>blog Without Sidebar</span>"
  -var header_small = "Blog"
  -var header_big = ""
  -var page_active = "Blog without Sidebar"
  -var page_parent = "Blog"
  include ../elements/_header-transparent
  // Page Content
  main.page-content
    include ../section/about/_section-breadcrumb
    include ../section/blog/_section-blog-5
    include ../section/blog/_section-blog-6
    include ../section/contact/_section-contact-2
    //include ../section/
  // Page Footer
  include ../elements/_footer-light-center
  // Rd Mailform result field
  .rd-mailform-validate